{
    "name": "differentdevelopment/different-core",
    "description": "",
    "keywords": [
        "laravel",
        "backpack",
        "different",
        "core",
        "extension"
    ],
    "homepage": "https://github.com/differentdevelopment/different-core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Different Fejleszt≈ë Kft.",
            "email": "php@different.hu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "backpack/permissionmanager": "^6.0",
        "illuminate/support": "^v10.13",
        "intervention/image": "^2.7",
        "spatie/laravel-activitylog": "^4.7.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.6.0",
        "orchestra/testbench": "^8.5.0",
        "phpunit/phpunit": "^10.2.0"
    },
    "autoload": {
        "files": [
            "src/app/Helpers/helpers.php"
        ],
        "psr-4": {
            "Different\\DifferentCore\\": "src",
            "Different\\DifferentCore\\Database\\Seeds\\": "src/database/seeds"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Different\\DifferentCore\\DifferentCoreServiceProvider"
            ],
            "aliases": {
                "DifferentCore": "Different\\DifferentCore\\DifferentCoreFacade"
            }
        }
    }
}
